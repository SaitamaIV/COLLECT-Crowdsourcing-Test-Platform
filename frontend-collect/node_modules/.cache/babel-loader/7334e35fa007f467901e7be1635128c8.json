           >\r\n            </el-input>\r\n          </el-form-item>\r\n        </el-col>\r\n        <el-col :span=\"24\">\r\n            <el-form-item label=\"漏洞描述\" prop=\"bugDescription\">\r\n              <el-input readonly\r\n                  v-model=\"ruleForm.bugDescription\"\r\n                  :autosize=\"{ minRows: 2, maxRows: 50 }\"\r\n                  type=\"textarea\"\r\n                  placeholder=\"漏洞描述信息\"\r\n                  style=\"width: 70%;margin-left: 2em\"\r\n              >\r\n              </el-input>\r\n            </el-form-item>\r\n        </el-col>\r\n        <el-col :span=\"24\">\r\n            <el-form-item label=\"复现步骤\" prop=\"bugRecurrentSteps\">\r\n              <el-input readonly\r\n                  v-model=\"ruleForm.bugRecurrentSteps\"\r\n                  :autosize=\"{ minRows: 2, maxRows: 50 }\"\r\n                  type=\"textarea\"\r\n                  placeholder=\"漏洞复现步骤\"\r\n                  style=\"width: 70%;margin-left: 2em\"\r\n              >\r\n              </el-input>\r\n            </el-form-item>\r\n        </el-col>\r\n        <el-col :span=\"24\">\r\n            <el-form-item label=\"设备信息\" prop=\"deviceInformation\">\r\n              <el-input readonly\r\n                  v-model=\"ruleForm.deviceInformation\"\r\n                  :autosize=\"{ minRows: 2, maxRows: 50 }\"\r\n                  type=\"textarea\"\r\n                  placeholder=\"设备详细信息\"\r\n                  style=\"width: 70%;margin-left: 2em\"\r\n              >\r\n              </el-input>\r\n            </el-form-item>\r\n        </el-col>\r\n        <el-col>\r\n          <el-form-item label=\"测试截图\"></el-form-item>\r\n        </el-col>\r\n        <el-col>\r\n          <template v-if=\"imgUrlList.length>0\">\r\n            <div class=\"demo-image__preview\">\r\n              <el-image v-if=\"imgUrlList.length>0\"\r\n                        style=\"width: 21em; height: 18em\"\r\n                        :src=\"imgUrlList[0]\"\r\n                        :preview-src-list=\"imgUrlList\"\r\n                        :initial-index=\"8\"\r\n                        fit=\"cover\"\r\n              />\r\n              <el-image v-if=\"imgUrlList.length>1\"\r\n                        style=\"width: 21em; height: 18em\"\r\n                        :src=\"imgUrlList[1]\"\r\n                        :preview-src-list=\"imgUrlList\"\r\n                        :initial-index=\"88\"\r\n                        fit=\"cover\"\r\n              />\r\n              <el-image v-if=\"imgUrlList.length>2\"\r\n                        style=\"width: 21em; height: 18em\"\r\n                        :src=\"imgUrlList[2]\"\r\n                        :preview-src-list=\"imgUrlList\"\r\n                        :initial-index=\"4\"\r\n                        fit=\"cover\"\r\n              />\r\n              <el-image v-if=\"imgUrlList.length>3\"\r\n                        style=\"width:21em; height: 18em\"\r\n                        :src=\"imgUrlList[3]\"\r\n                        :preview-src-list=\"imgUrlList\"\r\n                        :initial-index=\"8\"\r\n                        fit=\"cover\"\r\n              />\r\n            </div>\r\n          </template>\r\n        </el-col >\r\n        <el-col :span=\"24\" style=\"margin-top: 3em\">\r\n          <el-form-item label=\"报告评分\" prop=\"score\">\r\n            <el-rate style=\"margin-top: 0.7em\" v-model=\"this.ruleForm.score\" disabled show-score text-color=\"#ff9900\" score-template=\"{value} points\"></el-rate>\r\n          </el-form-item>\r\n        </el-col>\r\n      </el-row>\r\n      <el-form-item style=\"margin-top: 2em\">\r\n        <el-button type=\"primary\" @click=\"relationDetail()\">查看相似和协作报告</el-button>\r\n        <el-button @click=\"back()\">返回</el-button>\r\n      </el-form-item>\r\n    </el-form>\r\n    <span> </span>\r\n    <el-divider />\r\n    <span> </span>\r\n\r\n\r\n    <comment :key=\"commentKey\" @doSend=\"doSend(arguments)\" :label=\"label\" :avatar=\"avatar\" :placeholder=\"placeholder\" :minRows=\"minRows\" :maxRows=\"maxRows\"></comment>\r\n\r\n<!--    <div style=\"background-color: white;padding-top: 3em\">-->\r\n<!--      <p style=\"display: inline; margin-left: 3em\">点击评分</p>-->\r\n<!--      <el-rate-->\r\n<!--          style=\"background-color: white;display: inline;margin-right: 3em\"-->\r\n<!--          v-model=\"score\"-->\r\n<!--          show-text-->\r\n<!--          :colors=\"['#99A9BF', '#F7BA2A', '#FF9900']\">-->\r\n<!--      </el-rate>-->\r\n<!--      <button class=\"btn\" @click=\"display()\">提交</button>-->\r\n<!--    </div>-->\r\n\r\n\r\n\r\n<!--    <comment-->\r\n<!--        style=\"background-color: white\"-->\r\n<!--        :commentNum=commentNum-->\r\n<!--        :commentList=commentList-->\r\n<!--        @doSend=\"doSend($event)\"-->\r\n<!--    >-->\r\n<!--    </comment>-->\r\n\r\n\r\n\r\n  </div>\r\n</template>\r\n<script>\r\n// 一些基本格式的校验校验\r\nimport axios from \"axios\";\r\nimport Comment from '@/components/common/Comment';\r\nexport default {\r\n  data() {\r\n    return {\r\n      commentKey:0,\r\n      label:\"SVIP\",\r\n      placeholder:\"说点什么吧\",\r\n      minRows:4,\r\n      maxRows:4,\r\n      commentNum:2,\r\n      avatar:require('@/components/common/img/icon/avtar.png'),\r\n      commentList:[\r\n        {\r\n          cid:1,score:3,nickname:'花非花',\r\n\r\n          content:\"废话有点多系列但是覅哦啊二百噶尔而不v平伯平事情到了这一步，连丁二狗这个天不怕地不怕的家伙都有点心虚了。没想到，陈标子把这个事情算在了老李家，李家是村支书，而...\",\r\n          submitTime:'2019-9-23 17:36:02',\r\n        },\r\n        {\r\n          cid:2,score:5,nickname:'花花',\r\n          content:\"废话有点多系列但是覅哦啊二百噶尔而不v平伯平事情到了这一步，连丁二狗这个天不怕地不怕的家伙都有点心虚了。没想到，陈标子把这个事情算在了老李家，李家是村支书，而...\",\r\n          submitTime:'2019-9-23 17:36:02',\r\n        },\r\n\r\n      ],\r\n\r\n      form: {},\r\n      fid: '',\r\n      imgUrlList: [],\r\n      fileList: [],\r\n      dialogVisible:false,\r\n      base:'',\r\n      files:'',\r\n      uid: '',\r\n      mid: '',\r\n      ruleForm: {\r\n        title:'',\r\n        bugDescription: '',\r\n        bugRecurrentSteps:'',\r\n        deviceInformation:'',\r\n        score:''\r\n      },\r\n    };\r\n  },\r\n  mounted() {\r\n    //将上一级的界面保存取出来\r\n    this.uid=this.$route.query.uid\r\n    this.mid=this.$route.query.mid;\r\n    console.log(\"uid:\");\r\n    console.log(this.uid);\r\n    console.log(\"mid:\");\r\n    console.log(this.mid);\r\n    this.init();\r\n  },\r\n  computed: {},\r\n  methods: {\r\n    relationDetail(){\r\n      this.$router.push({\r\n        path:'/report',\r\n        query:{\r\n          fid:this.fid,\r\n        }\r\n      })\r\n    },\r\n    updata(event){\r\n      event.preventDefault()\r\n      let file=event.target.files[0] //根据事件源找到上传的file\r\n      let formData = new FormData();\r\n      formData.append('multipartFile',file);//键名要和后台一致\r\n      const instance=axios.create({\r\n        withCredentials: true\r\n      })\r\n      instance.post('/collect/file/upload',formData).then(res=>{\r\n        console.log(res.data.data)\r\n        this.exeUrl = res.data.data\r\n        // this.ruleForm.exeUrl=res.data\r\n        // console.log(this.ruleForm.exeUrl)\r\n      })\r\n    },\r\n    // 初始化\r\n    init() {\r\n      this.$http({\r\n        url: `fetchmission/searchreportbyids?uid=${this.uid}&mid=${this.mid}`,\r\n        method: \"get\"\r\n      }).then(({data}) => {\r\n        if(data.picture1!=null)\r\n          this.imgUrlList.push(data.picture1)\r\n        if(data.picture2!=null)\r\n          this.imgUrlList.push(data.picture2)\r\n        if(data.picture3!=null)\r\n          this.imgUrlList.push(data.picture3)\r\n        if(data.picture4!=null)\r\n          this.imgUrlList.push(data.picture4)\r\n        console.log(this.imgUrlList)\r\n        this.ruleForm = data;\r\n        this.fid=data.fid\r\n        console.log(this.ruleForm);\r\n      });\r\n    },\r\n    //发布评论\r\n    doSend(args){\r\n      let body={\r\n        \"submitTime\": Date.now(),\r\n        \"content\": args[0],\r\n        \"score\": args[1],\r\n      }\r\n      console.log(args[0])\r\n      console.log(body)\r\n      console.log(args[1])\r\n      this.$http({\r\n        url: `comment/submitcomment?fid=${this.fid}&uid=${this.uid}`,\r\n        method: \"post\",\r\n        data: body\r\n      }).then(({data}) => {\r\n        if(data && data.code === 0){\r\n          this.$message({\r\n            message: \"发布成功\",\r\n            type: \"success\",\r\n            duration: 1500,\r\n            })\r\n          this.commentKey+=1\r\n        }\r\n        else{\r\n          this.$message.error(data.msg);\r\n        }\r\n      });\r\n\r\n\r\n    },\r\n    // 返回\r\n    back() {\r\n      this.$router.push({\r\n        path:'/report',\r\n        query:{\r\n          mid:this.mid\r\n        }\r\n      })\r\n    },\r\n  },\r\n  components:{\r\n    Comment\r\n  }\r\n};\r\n</script>\r\n<style lang=\"scss\" scoped>\r\n</style>\r\n"]}]}